{'dec'@{   use #
    {'b'.{i}@{0}},
    {'b'.{c}@{
        {'b'['b'.{i}]@{1}},
        {'b'.{i}@'inc'#('b'.{i})}
    }},
    {'b'.{d}@{~'not'['b'['b'.{i}]]}},
    'whil',
    {'b'['b'.{i}]@{0}}
}},
{'inc'@{   use #
    {'b'.{i}@{0}},
    {'b'.{c}@{
        {'b'['b'.{i}]@{0}}, 
        {'b'.{i}@'inc'#('b'.{i})}
    }},
    {'b'.{d}@{~'b'['b'.{i}]}},
    'whil',
    {'b'['b'.{i}]@{1}}
}},
{'+'@{
    {'b'.{e}@{'a'@'inc'#'a'}},
    {'b'.{f}@'b'},
    'fo',
    {~'a'}
}},
{'-'@{
    {'b'.{e}@{'a'@'dec'#'a'}},
    {'b'.{f}@'b'},
    'fo',
    {~'a'}
}},
{'*'@{
    {:.{ret}@{0}},
    {'b'.{e}@{'*'.{ret}@('*'.{ret})+'a'}},
    {'b'.{f}@'b'},
    {'fo'},
    {~:.{ret}}
}},
{'/'@{
    {:.{ret}@{0}},
    {'b'.{c}@{
        {'a'@'a'-'b'},
        {'/'.{ret}@'inc'#'/'.{ret}}
    }},
    {'b'.{d}@{~'a'>='b'}},
    'whil',
    {~:.{ret}}
}},
{'%'@{
    {'b'.{c}@{'a'@'a'-'b'}},
    {'b'.{d}@{~'a'>='b'}},
    'whil',
    {~'a'}
}},
{'^'@{
    {:.{ret}@{1}},
    {'b'.{e}@{'^'.{ret}@('^'.{ret})*'a'}},
    {'b'.{f}@'b'},
    'fo',
    {~:.{ret}}
}},
{'!'@{
    {'!'.{ret}@{1}},
    {'b'.{e}@{'!'.{ret}@('!'.{ret})*'inc'#'b'.{f}}},
    {'b'.{f}@'b'},
    'fo',
    {'b'@'!'.{ret}},
    {~'b'}
}}