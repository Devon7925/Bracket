{'check'@{
    {{:.{ret}@{sucess}}if'a'[1]='b'},
    {{:.{ret}@{fail}}if'not'['a'[1]='b']},
    {:.{ret}@'a'[0]app{ - }app:.{ret}},
    {'print'#:.{ret}}
}},
{{{Dec              },{30} }check'dec'#{31}},
{{{Addition         },{111}}check{101}+{10}},
{{{Subtraction      },{91} }check{101}-{10}},
{{{Multiplication   },{110}}check{11}*{10}},
{{{Division         },{5}  }check{34}/{6}},
{{{Modulo           },{4}  }check{34}%{6}},
{{{Expodential      },{27} }check{3}^{3}},
{{{Factorial        },{24} }check'!'#{4}},
{{{bit shift L      },{56} }check{14}<<{2}},
{{{bit shift R      },{3}  }check{14}>>{2}}