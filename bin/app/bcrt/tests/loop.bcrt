'while'@{{'dowhile'}if{}b{}};
'dowhile'@{
    {'a'},
    {{'dowhile'}if{}b{}}
};
'whil'@{   is NOT an operation
    {'o'@'b'.{d}},
    {{'dowhil'}if'a'o'b'}
};
'dowhil'@{   is NOT an operation
    {'b'.{c}},
    {'o'@'b'.{d}},
    {{'dowhil'}if'a'o'b'}
};
'for'@{
    {:.{a}@'a'},
    {:.{b}@'b'},
    {'b'.{c}@{
        {'for'.{b}@'dec'#'for'.{b}},
        {'for'.{a}}
    }},
    {'b'.{d}@{~'not'[{1}['for'.{b}]]}},
    {'whil'}
};
'fo'@{   is NOT an operation
    {'b'.{c}@{
        {'b'.{f}@'dec'#'b'.{f}},
        {'b'.{e}}
    }},
    {'b'.{d}@{~'not'[{1}['b'.{f}]]}},
    {'whil'}
};